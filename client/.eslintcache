/zuares/Zuares1/Programing/Source Code/Javascript/ExpressJS/Dev At Viet/client/src/components/Cart/Cart.js",["68","69"],"import React, { useContext } from 'react';\nimport { GlobalState } from '../../GlobalState';\nimport ButtonRender from '../Products/ButtonRender';\n\nfunction Cart() {\n    const state = useContext(GlobalState);\n    const [cart, setCart] = state.userAPI.cart;\n    console.log(cart);\n\n    if (cart.length === 0) {\n        return (\n            <div className={`container py-8`} >\n                <h2 className={`font-bold`} >Empty Cart</h2>\n            </div>\n        )\n    }\n\n    return (\n        <div className={`container py-8 `} >\n            <div >\n\n                {\n                    cart.map(item => {\n                        return (\n                            <div className={`w-full flex gap-8 mt-4 `} >\n                                <div className={`w-1/2`}>\n                                    <img src={item.images.url} alt={item.images.url} className={`mx-auto w-full`} />\n                                </div>\n                                <div className={`w-1/2`} >\n\n                                    <div className={`flex justify-between`} >\n                                        <h2 className={`font-bold text-xl`} >{item.title}</h2>\n                                        <h3>{item.product_id}</h3>\n                                    </div>\n\n                                    <div className={`mt-4`} >\n                                        <span>$ {item.price}</span>\n                                    </div>\n\n                                    <div className={`mt-4`} >\n                                        <p>{item.description}</p>\n                                        <p>{item.content}</p>\n                                    </div>\n\n                                    <p className={`my-2`} >{item.sold}</p>\n                                    <div className={`flex gap-1 items-center text-white font-bold`} >\n                                        <button className={`w-8 py-0.5 bg-green-500`} >-</button>\n                                        <input type=\"text\" className={`w-8 h-8 border text-center text-black `} value={1} />\n                                        <button className={`w-8 py-0.5 bg-green-500`} >+</button>\n                                    </div>\n\n                                </div>\n                            </div>\n                        )\n                    })\n                }\n            </div>\n        </div>\n\n    );\n}\n\nexport default Cart;","/media/zuares/Zuares1/Programing/Source Code/Javascript/ExpressJS/Dev At Viet/client/src/api/ProductsAPI.js",[],"/media/zuares/Zuares1/Programing/Source Code/Javascript/ExpressJS/Dev At Viet/client/src/components/Products/ProductItem.js",["70"],"import { Link } from 'react-router-dom';\nimport ButtonRender from './ButtonRender';\nfunction ProductItem({ product, isAdmin }) {\n    const { images, title, price, description } = product;\n\n    return (\n        <div className={`bg-white shadow-lg relative `} style={{ width: `450px` }}  >\n            {\n                isAdmin && <input type=\"checkbox\" className={`absolute inset-0 w-6 h-6`} name=\"checked\" checked={product.checked} />\n            }\n            <img src={images.url} alt={images.url} className={`w-full object-cover h-60  rounded-t-sm `} />\n            <div className={`p-2 `} >\n\n                <div className={``}  >\n                    <div className={`flex w-full justify-between`} >\n                        <h2 className={`font-bold text-lg capitalize `} title={title} >{title}</h2>\n                        <span className={`text-yellow-500 font-bold`} >${price}</span>\n                    </div>\n                    <p>{description}</p>\n                </div>\n                <ButtonRender product={product} />\n\n            </div>\n        </div>\n    );\n}\n\nexport default ProductItem;","/m